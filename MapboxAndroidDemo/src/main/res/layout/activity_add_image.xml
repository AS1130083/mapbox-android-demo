<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:mapbox="http://schemas.android.com/apk/res-auto"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">
    <com.mapbox.mapboxsdk.maps.MapView
        android:id="@+id/mapView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        mapbox:mapbox_cameraZoomMax="5.99"
        mapbox:mapbox_cameraZoomMin="4"
        mapbox:mapbox_cameraTargetLat="41.874"
        mapbox:mapbox_cameraTargetLng="-75.789"
        mapbox:mapbox_cameraZoom="5"
        mapbox:mapbox_styleUrl="@string/mapbox_style_dark"/>
</LinearLayout>